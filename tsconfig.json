        "module": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "target": "es5",
        "outDir": "junk_js_files"
    },
    "include": [
        "app/**/*"
    ],
    "exclude": [
        "node_modules",
        "**/*.spec.ts"
    ]
}
